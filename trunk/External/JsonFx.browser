<browsers>
	<!-- http://msdn2.microsoft.com/en-us/library/ms228122.aspx -->
	<!-- http://forums.asp.net/thread/1176101.aspx -->
	
	<!-- IE/Firefox -->
    <browser id="JsonFx" parentID="Default" >
		<identification>
			<header name="Accept" match="application/json" />
		</identification>
		<capture>
			<userAgent match="^JsonFx/(?'version'(?'major'\d+)\.(?'minor'\d+)(\.(?'release'\d+))?(\.(?'build'\d+))?)\s*(?'verInfo'\w*)\s*(?'info'.*)$" />
		</capture>
		<capabilities>
			<capability name="browser" value="JsonFx" />
            <capability name="type" value="JsonFx${major}" />
			<capability name="version" value="${version}" />
			<capability name="majorversion" value="${major}" />
			<capability name="minorversion" value="${minor}" />
			<capability name="etc" value="${info}" />
			<capability name="verInfo" value="${verInfo}" />
			<capability name="ecmaScriptVersion" value="1.2" />
			<capability name="javaScript" value="true" />
			<capability name="preferredRenderingMime" value="application/json" />
			<capability name="preferredRequestEncoding" value="UTF-8" />
			<capability name="preferredResponseEncoding" value="UTF-8" />
			<capability name="supportsCss" value="true" />
			<capability name="supportsXmlHttp" value="true" />
			<capability name="supportsJson" value="true" />
			<capability name="supportsJsonML" value="true" />
			<capability name="tables" value="true" />
			<capability name="tagWriter"  value="JsonFx.UI.JsonXhtmlTextWriter, JsonFx" />
			<capability name="w3cDomVersion" value="1.0" />
		</capabilities>
	</browser>

	<!-- Opera -->
    <browser id="JsonFxOpera8" parentID="JsonFx">
        <identification>
            <capability name="etc" match="\bopera/8" />
        </identification>

        <capabilities>
			<capability name="preferredRenderingMime" value="text/plain" />
        </capabilities>
    </browser>

